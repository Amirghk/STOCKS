@page
@model STOCKS.Pages.DashBoardModel
@{
}
<div>
    <table class="table">
        <thead>
            <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Percent</th>
            </tr>            
        </thead>
        <tbody>
            <tr>
                <th>First</th>
                <th>1000</th>
                <th>3</th>
            </tr>
            <tr>
                <th>Second</th>
                <th>1000</th>
                <th>3</th>
            </tr>
            <tr>
                <th>Third</th>
                <th>1000</th>
                <th>3</th>
            </tr>

        </tbody>
    </table>


        <form>
  <div class="form-group">
    <label for="prices">Enter Price</label>
    <input class="form-control" id="prices" aria-describedby="emailHelp" placeholder="Price">
    <small id="emailHelp" class="form-text text-muted">Your desired Price</small>
  </div>
  <div class="form-group">
    <label for="count">Enter Count</label>
    <input type="password" class="form-control" id="count" placeholder="Count">
  </div>
  <br />
  
  <button type="submit" class="btn btn-primary">Buy</button>
  <button type="submit" class="btn btn-primary">Sell</button>
</form>


</div>